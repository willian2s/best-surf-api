{
  "name": "best-surf-api",
  "version": "1.0.0",
  "description": "Api of the surf meteorology application using StormGlass service",
  "main": "index.js",
  "repository": "git@github.com:willian2s/best-surf-api.git",
  "author": "Willian Silva <silva.williansantos@gmail.com>",
  "license": "MIT",
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api",
    "stormglass"
  ],
  "bugs": {
    "url": "https://github.com/willian2s/best-surf-api/issues"
  },
  "homepage": "https://github.com/willian2s/best-surf-api#readme",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "yarn test:unit && yarn test:functional",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "14.17.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  }
}
