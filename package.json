{
  "name": "forecast-api",
  "version": "1.0.0",
  "description": "Api of the surf meteorology application using StormGlass service",
  "main": "index.js",
  "repository": "git@github.com:willian2s/forecast-api.git",
  "author": "Willian Silva <silva.williansantos@gmail.com>",
  "license": "MIT",
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api",
    "stormglass"
  ],
  "bugs": {
    "url": "https://github.com/willian2s/forecast-api/issues"
  },
  "homepage": "https://github.com/willian2s/forecast-api#readme",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:dev": "dotenv -e .env -- ts-node-dev --transpile-only 'src/index.ts' | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "yarn lint && yarn style:check && yarn test:unit && yarn test:functional",
    "test:unit": "dotenv -e .env.test -- jest",
    "test:functional": "dotenv -e .env.test -- jest --projects ./test --runInBand",
    "test:cov": "jest --coverage"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.39",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.173",
    "@types/module-alias": "^2.0.1",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@types/nock": "^11.1.0",
    "@types/node": "14.17.11",
    "@types/node-cache": "^4.2.5",
    "@types/pino": "^6.3.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.32.0",
    "jest": "^27.1.0",
    "nock": "^13.1.3",
    "pino-pretty": "^7.0.0",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.1",
    "express-pino-logger": "^6.0.0",
    "express-rate-limit": "^5.3.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^6.0.5",
    "node-cache": "^5.1.2",
    "pino": "^6.13.2",
    "swagger-ui-express": "^4.1.6"
  }
}
